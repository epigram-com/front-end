{
  "manifest_version": 2,
  "name": "epigram",
  "version": "1.0",
  "description": "blocking the click bait for you by providing the answer",
  "permissions":["tabs","storage","https://www.facebook.com/*","http://www.facebook.com/*","background"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/epigram_icon_16.png",
    "32": "icons/epigram_icon_32.png",
    "48": "icons/epigram_icon_48.png",
    "128": "icons/epigram_icon_128.png"
  }
}